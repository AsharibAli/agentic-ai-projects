# OpenRouter API Key (get from https://openrouter.ai/settings/keys)
# OpenRouter provides access to many models, some are free!
OPENROUTER_API_KEY=your_openrouter_api_key_here

# OpenAI API Key (get from https://platform.openai.com/api-keys)
# Required for GPT models and embeddings
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (get from https://console.anthropic.com/)
# Required for Claude models
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google AI API Key (get from https://aistudio.google.com/app/apikey)
# Required for Gemini models
GOOGLE_API_KEY=your_google_api_key_here